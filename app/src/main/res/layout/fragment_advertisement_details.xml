<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.fragment.advertise.post_advertisement.AdvertisementDetailsFragment">

    <ImageView
        android:id="@+id/addImage"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_500sdp"
        android:layout_gravity="top|center"
        android:layout_marginBottom="@dimen/_102sdp"
        android:scaleType="fitXY" />


    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_120sdp">

        <FrameLayout
            android:id="@+id/classifiedDetailsBottom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/top_rounded_corner_shape"
            android:paddingTop="5dp"
            app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.core.widget.NestedScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:id="@+id/linear"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_18sdp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="@dimen/_12sdp">

                            <TextView
                                android:id="@+id/advertiseNameTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:ellipsize="end"
                                android:fontFamily="@font/sf_pro_semi_bold"
                                android:includeFontPadding="false"
                                android:maxLines="1"
                                android:text="Movers &amp; Packers"
                                android:textColor="#333333"
                                android:textSize="18sp"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/advertiseLocationTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:drawableLeft="@drawable/ic_location"
                                android:drawablePadding="5dp"
                                android:ellipsize="end"
                                android:fontFamily="@font/sf_pro_display_light"
                                android:gravity="center_vertical"
                                android:includeFontPadding="false"
                                android:maxLines="1"
                                android:text="New York 0852"
                                android:textColor="#707070"
                                android:textSize="12sp"
                                app:layout_constraintTop_toBottomOf="@+id/advertiseNameTv" />

                            <TextView
                                android:id="@+id/advertiseDateTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:drawableLeft="@drawable/ic_time_watch"
                                android:drawablePadding="5dp"
                                android:ellipsize="end"
                                android:fontFamily="@font/sf_pro_display_light"
                                android:includeFontPadding="false"
                                android:maxLines="1"
                                android:text="From Mar 15,2022 To Mar 21,2022"
                                android:textColor="#707070"
                                android:textSize="12sp"
                                app:layout_constraintTop_toBottomOf="@+id/advertiseLocationTv" />

                            <TextView
                                android:id="@+id/advertiseLinkTv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4sdp"
                                android:drawableLeft="@drawable/link_icon"
                                android:drawablePadding="5dp"
                                android:ellipsize="end"
                                android:fontFamily="@font/sf_pro_medium"
                                android:gravity="center_vertical"
                                android:includeFontPadding="false"
                                android:maxLines="1"
                                android:text="@string/tap_now"
                                android:textColor="#124c94"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/advertiseDateTv" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/sellerInformationLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/compnyDetails"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:fontFamily="@font/sf_pro_regular"
                                android:text="Company Description"
                                android:textColor="#888282"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <com.chinalwb.are.render.AreTextView
                                android:id="@+id/companyDescTv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/sf_pro_regular"
                                android:text="Home Packers Movers is a trusted name in Delhi and nearby region in the field of packing and transporting of goods. Our company deals with the business of Packers and movers in Delhi and is supported by a team of professionals with skills in this field."
                                android:textColor="#333333"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="@id/compnyDetails"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toStartOf="@id/compnyDetails"
                                app:layout_constraintTop_toBottomOf="@id/compnyDetails" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

