<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_registration_graph"
    app:startDestination="@id/basicDetailsFragment">

    <fragment
        android:id="@+id/basicDetailsFragment"
        android:name="com.aaonri.app.ui.authentication.register.fragment.BasicDetailsFragment"
        android:label="fragment_basic_details"
        tools:layout="@layout/fragment_basic_details" >
        <action
            android:id="@+id/action_basicDetailsFragment2_to_addressDetailsFragment2"
            app:destination="@id/addressDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/addressDetailsFragment"
        android:name="com.aaonri.app.ui.authentication.register.fragment.AddressDetailsFragment"
        android:label="fragment_address_details"
        tools:layout="@layout/fragment_address_details" >
        <action
            android:id="@+id/action_addressDetailsFragment2_to_servicesCategoryFragment2"
            app:destination="@id/servicesCategoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/locationDetailsFragment"
        android:name="com.aaonri.app.ui.authentication.register.fragment.LocationDetailsFragment"
        android:label="fragment_location_details"
        tools:layout="@layout/fragment_location_details" />
    <fragment
        android:id="@+id/servicesCategoryFragment"
        android:name="com.aaonri.app.ui.authentication.register.fragment.ServicesCategoryFragment"
        android:label="fragment_services_category"
        tools:layout="@layout/fragment_services_category" >
        <action
            android:id="@+id/action_servicesCategoryFragment2_to_locationDetailsFragment2"
            app:destination="@id/locationDetailsFragment" />
    </fragment>
</navigation>