<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_nav_graph"
    app:startDestination="@id/homeScreenFragment">

    <fragment
        android:id="@+id/homeScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.HomeScreenFragment"
        android:label="fragment_home_screen"
        tools:layout="@layout/fragment_home_screen">
        <action
            android:id="@+id/action_homeScreenFragment_to_classifiedScreenFragment"
            app:destination="@id/classifiedScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_eventScreenFragment"
            app:destination="@id/eventScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_classifiedDetailsFragment"
            app:destination="@id/classifiedDetailsFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_eventDetailsScreenFragment"
            app:destination="@id/eventDetailsScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/classifiedScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.classified.ClassifiedScreenFragment"
        android:label="fragment_classified_screen"
        tools:layout="@layout/fragment_classified_screen">
        <action
            android:id="@+id/action_classifiedScreenFragment_to_shopScreenFragment"
            app:destination="@id/shopScreenFragment" />
        <action
            android:id="@+id/action_classifiedScreenFragment_to_classifiedFilterFragmentBottom"
            app:destination="@id/classifiedFilterFragmentBottom"
            app:enterAnim="@anim/slide_up"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_classifiedScreenFragment_to_classifiedDetailsFragment"
            app:destination="@id/classifiedDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/shopScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.ShopScreenFragment"
        android:label="fragment_shop_screen"
        tools:layout="@layout/fragment_shop_screen">
        <action
            android:id="@+id/action_shopScreenFragment_to_advertiseScreenFragment"
            app:destination="@id/advertiseScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/advertiseScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.AdvertiseScreenFragment"
        android:label="fragment_advertise_screen"
        tools:layout="@layout/fragment_advertise_screen">
        <action
            android:id="@+id/action_advertiseScreenFragment_to_moreScreenFragment"
            app:destination="@id/moreScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/moreScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.MoreScreenFragment"
        android:label="fragment_more_screen"
        tools:layout="@layout/fragment_more_screen" />
    <fragment
        android:id="@+id/classifiedFilterFragmentBottom"
        android:name="com.aaonri.app.ui.dashboard.fragment.classified.fragment.ClassifiedFilterFragmentBottom"
        android:label="fragment_classified_filter"
        tools:layout="@layout/fragment_classified_filter">
        <action
            android:id="@+id/action_classifiedFilterFragmentBottom_to_selectClassifiedSubCategoryBottom2"
            app:destination="@id/selectClassifiedSubCategoryBottom2" />
        <action
            android:id="@+id/action_classifiedFilterFragmentBottom_to_selectClassifiedCategoryBottom2"
            app:destination="@id/selectClassifiedCategoryBottom2" />
        <action
            android:id="@+id/action_classifiedFilterFragmentBottom_to_classifiedScreenFragment"
            app:destination="@id/classifiedScreenFragment"
            app:popUpTo="@id/classifiedScreenFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/classifiedDetailsFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.classified.fragment.ClassifiedDetailsFragment"
        android:label="fragment_classified_details"
        tools:layout="@layout/fragment_classified_details">
        <action
            android:id="@+id/action_classifiedDetailsFragment_to_updateDeleteClassifiedBottom"
            app:destination="@id/updateDeleteClassifiedBottom" />
        <argument
            android:name="addId"
            app:argType="integer" />
        <argument
            android:name="isMyClassifiedScreen"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/eventScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.event.EventScreenFragment"
        android:label="fragment_event_screen"
        tools:layout="@layout/fragment_event_screen">
        <action
            android:id="@+id/action_eventScreenFragment_to_eventDetailsScreenFragment"
            app:destination="@id/eventDetailsScreenFragment" />
        <action
            android:id="@+id/action_eventScreenFragment_to_eventFilterScreenFragment"
            app:destination="@id/eventFilterScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/eventDetailsScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.event.post_event.EventDetailsScreenFragment"
        android:label="fragment_event_details"
        tools:layout="@layout/fragment_event_details">
        <argument
            android:name="eventId"
            app:argType="integer" />
        <action
            android:id="@+id/action_eventDetailsScreenFragment_to_updateDeleteClassifiedBottom"
            app:destination="@id/updateDeleteClassifiedBottom" />
    </fragment>

    <dialog
        android:id="@+id/updateDeleteClassifiedBottom"
        android:name="com.aaonri.app.ui.dashboard.fragment.classified.fragment.UpdateDeleteClassifiedBottom"
        android:label="fragment_update_delete_classified_bottom"
        tools:layout="@layout/fragment_update_delete_classified_bottom">
        <argument
            android:name="addId"
            app:argType="integer" />
        <argument
            android:name="isClassifiedUpdate"
            app:argType="boolean" />
        <action
            android:id="@+id/action_updateDeleteClassifiedBottom_to_eventScreenFragment"
            app:destination="@id/eventScreenFragment" />
    </dialog>
    <dialog
        android:id="@+id/selectClassifiedCategoryBottom2"
        android:name="com.aaonri.app.ui.dashboard.fragment.classified.post_classified.SelectClassifiedCategoryBottom"
        android:label="fragment_select_classified_category_bottom"
        tools:layout="@layout/fragment_select_classified_category_bottom" />
    <dialog
        android:id="@+id/selectClassifiedSubCategoryBottom2"
        android:name="com.aaonri.app.ui.dashboard.fragment.classified.fragment.SelectClassifiedSubCategoryBottom"
        android:label="fragment_select_classified_sub_category_bottom"
        tools:layout="@layout/fragment_select_classified_sub_category_bottom" />
    <fragment
        android:id="@+id/eventFilterScreenFragment"
        android:name="com.aaonri.app.ui.dashboard.fragment.event.fragment.EventFilterScreenFragment"
        android:label="fragment_event_filter_screen"
        tools:layout="@layout/fragment_event_filter_screen">
        <action
            android:id="@+id/action_eventFilterScreenFragment_to_eventScreenFragment"
            app:destination="@id/eventScreenFragment"
            app:popUpTo="@id/eventScreenFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_eventFilterScreenFragment_to_selectFilterEventCityBottom"
            app:destination="@id/selectFilterEventCityBottom" />
    </fragment>
    <dialog
        android:id="@+id/selectFilterEventCityBottom"
        android:name="com.aaonri.app.ui.dashboard.fragment.event.fragment.SelectFilterEventCityBottom"
        android:label="fragment_select_filter_event_city_bottom"
        tools:layout="@layout/fragment_select_filter_event_city_bottom" />
</navigation>